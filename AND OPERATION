ORG 0000H
SETB P0.0;TO SET PORT PINS
SETB P0.1
SETB P0.2
READ:MOV C,P0.0;TO MOVE P0.0 TO C
CPL C ;COMPLEMENT C
MOV 00H,C ;TO MOVE C TO 00H
MOV C,P0.1 ; TO MOVE P0.1 TO C
CPL C ;COMPLEMENT C
ANL C,00H ;IMPLEMENT THE AND OPERATION
CPL C ;COMPLEMENT C
MOV P0.2,C ;TO MOVE C TO P0.2
SJMP READ ; JUMP TO READ
END 

